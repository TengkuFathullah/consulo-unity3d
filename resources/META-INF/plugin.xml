<idea-plugin>
	<id>org.mustbe.consulo.unity3d</id>
	<name>Unity3D</name>
	<vendor>Must-Be</vendor>
	<version>SNAPSHOT</version>

	<depends>org.mustbe.consulo.dotnet.core</depends>
	<depends>org.mustbe.consulo.dotnet.mono</depends>
	<depends>org.mustbe.consulo.csharp</depends>

	<application-components>
		<component>
			<implementation-class>org.mustbe.consulo.unity3d.run.debugger.UnityPlayerService</implementation-class>
		</component>
	</application-components>

	<extensions defaultExtensionNs="com.intellij">
		<sdkType implementation="org.mustbe.consulo.unity3d.bundle.Unity3dBundleType" />
		<bundledSdkProvider implementation="org.mustbe.consulo.unity3d.bundle.Unity3dBundleProvider" />

		<configurationType implementation="org.mustbe.consulo.unity3d.run.UnityRemoteDebugApplication" />

		<programRunner implementation="org.mustbe.consulo.unity3d.run.UnityRemoteDebugRunner" />
		<moduleExtensionProvider
				key="unity3d"
				name="Unity3D"
				icon="/icons/unity3d.png"
				immutableClass="org.mustbe.consulo.unity3d.module.Unity3dModuleExtension"
				mutableClass="org.mustbe.consulo.unity3d.module.Unity3dMutableModuleExtension"/>

		<moduleExtensionProvider
				key="unity3d-csharp"
				parentKey="unity3d"
				name="C#"
				icon="/icons/unity3d.png"
				immutableClass="org.mustbe.consulo.unity3d.csharp.module.extension.Unity3dCSharpModuleExtension"
				mutableClass="org.mustbe.consulo.unity3d.csharp.module.extension.Unity3dCSharpMutableModuleExtension"/>

		<newModuleBuilder implementation="org.mustbe.consulo.unity3d.ide.newProjectOrModule.Unity3dNewModuleBuilder" />
	</extensions>
</idea-plugin>